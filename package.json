{
  "private": true,
  "name": "website",
  "description": "My personal website.",
  "version": "1.0.0",
  "scripts": {
    "clean": "del .next out",
    "build": "npm run clean && next build",
    "export": "next export",
    "postexport": "cp -r .github/. out",
    "start": "next start -p 6060",
    "deploy": "npm run build && npm run export && git-directory-deploy --directory out",
